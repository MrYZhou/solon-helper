{
    "name": "solon-helper",
    "displayName": "Solon Helper",
    "description": "%description%",
    "version": "1.24.82",
    "engines": {
      "vscode": "^1.92.0"
    },
    "categories": [
      "Programming Languages",
      "Linters"
    ],
    "keywords": [
      "Maven",
      "Java"
    ],
    "publisher": "larry",
    "repository": {
      "type": "git",
      "url": "https://github.com/MrYZhou/solon-helper"
    },
    "bugs": {
      "url": "https://github.com/MrYZhou/solon-helper/issues"
    },
    "homepage": "https://github.com/MrYZhou/solon-helper",
    "preview": false,
    "icon": "favicon.ico",
    "activationEvents": [
      "workspaceContains:**/app.yml",
      "workspaceContains:**/app.yaml"
    ],
    "main": "./main.js",
    "contributes": {
      "commands": [
        {
          "command": "solon.initSolonProject",
          "title": "%solon.createProject%",
          "category": "Solon"
        }
      ],
      "keybindings": [
        {
          "key": "alt+shift+s",
          "command": "solon.initSolonProject"
        }
      ],
      "configuration": {
        "properties": {
          "solon-helper.customPath": {
            "type": "object",
            "markdownDescription": "自定义快捷创建路径",
            "patternProperties": {
              "^[a-zA-Z|a-zA-Z0-9|一-龥]*$": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      }
    },
    "scripts": {
      "compile": "tsc -p ./",
      "watch": "webpack --mode development --watch",
      "test": "npm run compile && node ./out/test/index.js",
      "lint": "eslint -c .eslintrc.js --ext .ts ./src",
      "vscode:prepublish": "webpack --mode production"
    },
    "devDependencies": {
      "@types/glob": "^8.1.0",
      "@types/mocha": "^10.0.1",
      "@types/node": "16.x",
      "@types/vscode": "^1.92.0",
      "@typescript-eslint/eslint-plugin": "^5.45.0",
      "@typescript-eslint/parser": "^5.45.0",
      "@vscode/test-electron": "^2.2.0",
      "eslint": "^8.39.0",
      "glob": "^8.1.0",
      "mocha": "^10.2.0",
      "ts-loader": "^9.5.1",
      "typescript": "^5.0.4",
      "webpack": "^5.93.0",
      "webpack-cli": "^5.1.4"
    },
    "dependencies": {
      "adm-zip": "^0.5.14",
      "js-yaml": "^4.1.0",
      "node-fetch": "^3.3.2"
    }
  }
  